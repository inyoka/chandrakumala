            <% if(currentUser){ %>
            <nav class="navbar navbar-expand bg-dark container">
                <div class="navbar-nav d-flex justify-content-between btn-group rounded w-100" role="group" aria-label="Basic example">
                    <div>
                        <a class="nav-item btn btn-info mx-1" href="/" title="Splash page"><i class="fas fa-home"></i></a>

                    </div>
                    <div>
                        <% if(currentUser){ %>  
                            <a class="nav-item btn btn-info mx-1" href="/slides/edit"><i class="fas fa-pen"title="Edit page"></i></a>
                            <a class="nav-item btn btn-info mx-1" href="/slides"><i class="fas fa-undo" title="Don't save"></i></a>
                        <% } %>
                    </div>
                    <div>
                        <% if(!currentUser){ %>  
                        <a class="nav-item btn btn-primary" href="/auth/login"><i class="fas fa-sign-in-alt"></i></a>
                        <a class="nav-item btn btn-success mx-1" href="/auth/register"><i class="fas fa-user-plus"></i></a>

                        <% } else { %>  
                        <a class="nav-item btn btn-success mx-1" href="/auth/register" title="New user"><i class="fas fa-user-plus"></i></a>
                        <a class="nav-item btn btn-warning" href="/auth/logout" title="Log Out"><i class="fas fa-sign-out-alt"></i></a>
                        <% } %>
                    </div>
                </div>
            </nav>
            <h1><% date %></h1>
            
            <% } %>


            <div class="container">
